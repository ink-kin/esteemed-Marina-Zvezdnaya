[
  { "id": 8851, "type": "message", "date": "2021-08-23T15:23:32", "from": "Автор", "icon": "А", "text": "Есть стихотворение: Чистая Единая Любовь есть Я ?" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Чистая Единая Любовь есть Я." },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Со Мной всегда Мои Друзья!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Каждый мой выбор и каждое моё решенье" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Приносят каждому только улучшенье!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Вечную Жизнь СВОЮ принимаю," },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Ярким Огнём всё время пылаю!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Только Любовь Я Тебе посылаю!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Верю в Себя и всё понимаю!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Лучшее становится реальностью легко," },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Знаю, что кругом всё уже великолепно!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Божья Любовь есть наша Защита!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Святая Дорога Мне сейчас открыта!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Великолепно провожу каждый свой день Я," },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Верю и знаю, что ярко сияет Моя Звезда!" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "В Мире, Гармонии всегда пребываю" },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "И только благотворные свои плоды собираю." },
  { "id": 8887, "type": "message", "date": "2021-08-24T15:11:38", "from": "Марина", "icon": "М", "text": "Благодарю" }
